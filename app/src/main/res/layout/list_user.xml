<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="90dp"
    android:orientation="horizontal"
    android:background="@drawable/rounded_list"
    android:layout_marginBottom="10dp"
    >
    <TextView
        android:id="@+id/tv_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:visibility="gone"

        />
    <!--android:foreground="@drawable/rounded_image"-->
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="32dp"
        android:layout_marginStart="32dp">
        <ImageView
            android:id="@+id/img_staf"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:foreground="@drawable/rounded_image"
            android:layout_gravity="center"
            android:layout_marginStart="16dp"
            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:layout_marginStart="16dp"
            >

            <TextView
                android:id="@+id/tv_namaS"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bocil Kematian"
                android:fontFamily="@font/poppins_medium"
                android:textColor="#000"
                android:fontSize="16sp"

                />
            <TextView
                android:id="@+id/tv_jabatan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Email"
                android:fontFamily="@font/poppins_medium"
                android:textColor="#686777"
                android:fontSize="12sp"


                />

        </LinearLayout>
    </LinearLayout>




</LinearLayout>